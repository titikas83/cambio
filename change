pre=float(input("ingrese el precio del articulo: "))
pag=float(input("'Â¿Cuanto ha pagado el cliente? "))

cambio=pag-pre
faltante=pre-pag

if (cambio<0):
    print("Falta dinero en el pago. El dinero es ",faltante)
    
elif (cambio==0):
    print("Se ha pagado exacto. No es necesario dar cambio")
    
if (cambio>0):
    print("El cambio a dar es ",cambio)
